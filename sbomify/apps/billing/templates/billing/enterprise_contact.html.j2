{% extends "core/dashboard_base.html.j2" %}
{% load static %}
{% block title %}sbomify Enterprise Contact{% endblock %}
{% block head %}
    {{ block.super }}
    {% if turnstile_site_key and not debug %}
        <script src="https://challenges.cloudflare.com/turnstile/v0/api.js"
                async
                defer></script>
    {% endif %}
{% endblock %}
{% block content %}
    <div x-cloak
        x-data="{ init() { const backButton = this.$el.querySelector('.btn-outline-secondary'); if (backButton) { backButton.href = '{% url "core:dashboard" %}'; backButton.innerHTML = '<i class=\"
        fas
        fa-arrow-left
        me-2
        "
        /i
        Back
        to
        Dashboard
        '
        ">{% include "billing/_enterprise_contact_form.html.j2" with form=form %}</div>
{% endblock %}
{% block scripts %}{{ block.super }}{% endblock %}
