{% extends "core/dashboard_base.html.j2" %}
{% load static %}
{% block title %}sbomify Enterprise Contact{% endblock %}
{% block head %}
    {{ block.super }}
    {% if turnstile_site_key and not debug %}
        <script src="https://challenges.cloudflare.com/turnstile/v0/api.js"
                async
                defer></script>
    {% endif %}
{% endblock %}
{% block content %}
    {% url 'core:dashboard' as dashboard_url %}
    {% include "billing/_enterprise_contact_form.html.j2" with form=form back_url=dashboard_url back_text="Back to Dashboard" %}
{% endblock %}
{% block scripts %}
    {{ block.super }}
    {% include "billing/_turnstile_scripts.html.j2" %}
{% endblock %}
